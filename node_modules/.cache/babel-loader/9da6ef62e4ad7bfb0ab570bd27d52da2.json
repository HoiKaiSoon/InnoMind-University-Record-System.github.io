token","mounted","findAll","methods","page","then","getHostelNameById","handleSelectionChange","val","onSubmit","clear","add","operator","message","type","update","selectById","handleSizeChange","handleCurrentChange","deleteById","confirmButtonText","cancelButtonText","catch","deleteByIds","handleAvatarSuccess","res","file","beforeAvatarUpload","watch"],"sourceRoot":"src/views/tenant","sources":["index.vue"],"sourcesContent":["<template>\n  <div class=\"app-container\">\n\n    <!--搜索表单-->\n    <el-form :inline=\"true\" :model=\"searchTenant\" class=\"demo-form-inline\">\n      <el-form-item label=\"姓名\">\n        <el-input\n          v-model=\"searchTenant.name\"\n          placeholder=\"请输入住户姓名\"\n        ></el-input>\n      </el-form-item>\n      <el-form-item label=\"性别\">\n        <el-select v-model=\"searchTenant.gender\" placeholder=\"请选择\">\n          <el-option label=\"男\" value=\"1\"></el-option>\n          <el-option label=\"女\" value=\"2\"></el-option>\n        </el-select>\n      </el-form-item>\n\n      <el-form-item label=\"入住时间\">\n        <el-date-picker\n          v-model=\"entrydate\"\n          clearable\n          value-format=\"yyyy-MM-dd\"\n          type=\"daterange\"\n          placeholder=\"选择日期\"\n          range-separator=\"至\"\n          start-placeholder=\"开始日期\"\n          end-placeholder=\"结束日期\"\n          style=\"width: 400px; margin-left: 20px\"\n        ></el-date-picker>\n      </el-form-item>\n\n      <el-form-item>\n        <el-button type=\"primary\" @click=\"onSubmit\">查询</el-button>\n         <el-button type=\"info\" @click=\"clear\">清空</el-button>\n      </el-form-item>\n    </el-form>\n\n    <!--按钮-->\n    <el-row>\n      <el-button type=\"danger\" size=\"medium\" @click=\"deleteByIds\">- 批量删除</el-button>\n      <el-button type=\"primary\" size=\"medium\" @click=\"dialogVisible = true; tenant = { image: ''};\" >+ 新增住户</el-button>\n    </el-row>\n\n    <!--添加数据对话框表单-->\n    <el-dialog ref=\"form\" title=\"新增住户\" :visible.sync=\"dialogVisible\" width=\"30%\">\n      <el-form ref=\"form\" :model=\"tenant\" label-width=\"80px\" size=\"mini\">\n        <el-form-item label=\"住户姓名\">\n          <el-input v-model=\"tenant.name\"></el-input>\n        </el-form-item>\n\n        <el-form-item label=\"头像\">\n          <el-upload\n            class=\"avatar-uploader\"\n            action=\"/api/upload\"\n            :headers=\"token\"\n            name=\"image\"\n            :show-file-list=\"false\"\n            :on-success=\"handleAvatarSuccess\"\n            :before-upload=\"beforeAvatarUpload\"\n          >\n            <img v-if=\"tenant.image\" :src=\"tenant.image\" class=\"avatar\" />\n            <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\n          </el-upload>\n        </el-form-item>\n\n        <el-form-item label=\"性别\" >\n          <el-select v-model=\"tenant.gender\" placeholder=\"请选择\" style=\"width:100%\" >\n             <el-option\n              v-for=\"item in genderList\"\n              :key=\"item.value\"\n              :label=\"item.name\"\n              :value=\"item.id\"\n            />\n          </el-select>\n        </el-form-item>\n\n        <el-form-item label=\"住户手机号\">\n          <el-input v-model=\"tenant.phone\"></el-input>\n        </el-form-item>\n\n        <el-form-item label=\"宿舍名称\">\n          <el-select v-model=\"tenant.hostelId\" placeholder=\"请选择\" value-key=\"tenant.hostel\" style=\"width:100%\">\n            <el-option\n              v-for=\"item in hostelList\"\n              :key=\"item.value\"\n              :label=\"item.name\"\n              :value=\"item.id\"\n            />\n          </el-select>\n        </el-form-item>\n\n        <el-form-item label=\"入住日期\">\n          <el-date-picker\n            v-model=\"tenant.entrydate\"\n            clearable\n            type=\"date\"\n            placeholder=\"选择日期\"\n            size=\"small\"\n            style=\"width:100%\"\n          ></el-date-picker>\n        </el-form-item>\n\n        <el-form-item>\n          <el-button type=\"primary\" @click=\"add\">提交</el-button>\n          <el-button @click=\"dialogVisible = false\">取消</el-button>\n        </el-form-item>\n      </el-form>\n    </el-dialog>\n\n    <br>\n    <!--表格-->\n    <template>\n      <el-table :data=\"tableData\" style=\"width: 100%\" border @selection-change=\"handleSelectionChange\">\n        <el-table-column type=\"selection\" width=\"55\"  align=\"center\"></el-table-column>\n        <el-table-column  prop=\"name\"  label=\"住户姓名\"  align=\"center\"></el-table-column>\n        <el-table-column prop=\"image\" label=\"头像\" align=\"center\">\n          <template slot-scope=\"{ row }\">\n            <el-image style=\"width: auto; height: 40px; border: none; cursor: pointer\" :src=\"row.image\"></el-image>\n          </template>\n        </el-table-column>\n\n        <el-table-column align=\"center\" label=\"性别\">\n          <template slot-scope=\"scope\">\n            <span style=\"margin-right: 10px\">\n            {{scope.row.gender == \"1\" ? \"男\" : \"女\"}}</span>\n          </template>\n        </el-table-column>\n\n        <el-table-column  prop=\"phone\"  label=\"住户手机号\"  align=\"center\"></el-table-column>\n        <el-table-column prop=\"hostelId\" label=\"宿舍名称\" align=\"center\">\n          <template slot-scope=\"{ row }\">\n            {{ getHostelNameById(row.hostelId) }}\n          </template>\n        </el-table-column>\n        <el-table-column align=\"center\" label=\"入住日期\">\n          <template slot-scope=\"scope\">\n            {{ scope.row.entrydate }}\n          </template>\n        </el-table-column>\n        <el-table-column align=\"center\" label=\"最后操作时间\">\n          <template slot-scope=\"scope\">\n            {{scope.row.updateTime ? scope.row.updateTime.replace('T',' '):''}}\n          </template>\n        </el-table-column>\n\n        <el-table-column align=\"center\" label=\"操作\">\n          <template slot-scope=\"scope\">\n            <el-button type=\"primary\" size=\"small\" @click=\"update(scope.row.id)\">编辑</el-button>\n            <el-button type=\"danger\" size=\"small\" @click=\"deleteById(scope.row.id)\">删除</el-button>\n          </template>\n        </el-table-column>\n      </el-table>\n    </template>\n\n    <!--分页工具条-->\n    <el-pagination\n      @size-change=\"handleSizeChange\"\n      @current-change=\"handleCurrentChange\"\n      :background=\"background\"\n      :current-page=\"currentPage\"\n      :page-sizes=\"[5, 10, 15, 20]\"\n      :page-size=\"5\"\n      layout=\"total, sizes, prev, pager, next, jumper\"\n      :total=\"totalCount\">\n    </el-pagination>\n  </div>\n</template>\n\n<script>\nimport { page, add, update, deleteById, selectById } from \"@/api/tenant.js\";\nimport { findAll } from \"@/api/hostel.js\";\nimport { getToken } from '@/utils/auth';\n\nexport default {\n  data() {\n    return {\n      background: true,\n      // 每页显示的条数\n      pageSize: 5,\n      // 总记录数\n      totalCount: 100,\n      // 当前页码\n      currentPage: 1,\n      // 添加数据对话框是否展示的标记\n      dialogVisible: false,\n      // 品牌模型数据\n      searchTenant: {\n        name: \"\",\n        gender: \"\",\n      },\n      tenant: {\n        image: \"\",\n        hostelId: null,\n      },\n      hostelList: [],\n      genderList: [{id: 1,name: \"男\"},{id: 2,name: \"女\"}],\n\n      beginTime: \"\",\n      endTime: \"\",\n      entrydate: \"\",\n\n      // 被选中的id数组\n      selectedIds: [],\n      // 复选框选中数据集合\n      multipleSelection: [],\n      // 表格数据\n      tableData: [],\n      token: {token: getToken()}\n    };\n  },\n\n  mounted() {\n    this.page();\n    //当页面加载完成后，发送异步请求，获取数据\n    findAll().then((result) => {\n      this.hostelList = result.data.data;\n    });\n  },\n  \n  methods: {\n    // 查询分页数据\n    page() {\n      page(\n        this.searchTenant.name,\n        this.searchTenant.gender,\n        this.beginTime,\n        this.endTime,\n        this.currentPage,\n        this.pageSize\n      ).then((res) => {\n        this.totalCount = res.data.data.total;\n        this.tableData = res.data.data.rows;\n      });\n    },\n\n    getHostelNameById(hostelId) {\n    const hostel = this.hostelList.find(hostel => hostel.id === hostelId);\n    return hostel ? hostel.name : '';\n  },\n\n    // 复选框选中后执行的方法\n    handleSelectionChange(val) {\n      this.multipleSelection = val;\n    },\n\n    // 查询方法\n    onSubmit() {\n      this.currentPage = 1;\n      this.page();\n    },\n\n    clear(){\n      this.searchTenant = {name: \"\", gender: \"\"};\n      this.beginTime = \"\",\n      this.endTime = \"\";\n      this.entrydate = \"\";\n      this.page();\n    },\n    // 添加数据\n    add() {\n      let operator;\n\n      if (this.tenant.id) {\n        //修改\n        operator = update(this.tenant);\n      } else {\n        operator = add(this.tenant);\n      }\n\n      operator.then((resp) => {\n        if (resp.data.code == \"1\") {\n          this.dialogVisible = false;\n          this.page();\n          this.$message({ message: \"恭喜你，保存成功\", type: \"success\" });\n          this.tenant = { image: \"\" };\n        } else {\n          this.$message.error(resp.data.msg);\n        }\n      });\n    },\n    update(id) {\n      //1. 打开窗口\n      this.dialogVisible = true;\n      //2. 发送请求\n\n      selectById(id).then((result) => {\n        if (result.data.code == 1) {\n          this.tenant = result.data.data;\n          this.tenant;\n        }\n      });\n    },\n\n    \n    //分页\n    handleSizeChange(val) {\n      // 重新设置每页显示的条数\n      this.pageSize = val;\n      this.page();\n    },\n\n    handleCurrentChange(val) {\n      // 重新设置当前页码\n      this.currentPage = val;\n      this.page();\n    },\n\n\n    //删除员工信息\n    deleteById(id){\n      this.$confirm(\"此操作将删除该数据, 是否继续?\", \"提示\", {\n        confirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n        type: \"warning\",\n      }).then(() => {\n          //2. 发送AJAX请求\n          deleteById(id).then((resp) => {\n            if (resp.data.code == 1) {\n              //删除成功\n              this.$message.success(\"恭喜你，删除成功\");\n              this.page();\n            } else {\n              this.$message.error(resp.data.msg);\n            }\n          });\n      }).catch(() => {\n          //用户点击取消按钮\n          this.$message.info(\"已取消删除\");\n        });\n    },\n\n\n    // 批量删除员工信息\n    deleteByIds() {\n      // 弹出确认提示框\n      this.$confirm(\"此操作将删除该数据, 是否继续?\", \"提示\", {\n        confirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n        type: \"warning\",\n      }).then(() => {\n          //用户点击确认按钮\n          //1. 创建id数组, 从 this.multipleSelection 获取即可\n          for (let i = 0; i < this.multipleSelection.length; i++) {\n            this.selectedIds[i] = this.multipleSelection[i].id;\n          }\n\n          //2. 发送AJAX请求\n          deleteById(this.selectedIds).then((resp) => {\n            if (resp.data.code == \"1\") {\n              //删除成功\n              this.$message.success(\"恭喜你，删除成功\");\n              this.page();\n            } else {\n              this.$message.error(resp.data.msg);\n            }\n          });\n      }).catch(() => {\n          //用户点击取消按钮\n          this.$message.info(\"已取消删除\");\n        });\n    },\n\n\n    //文件上传相关\n    handleAvatarSuccess(res, file) {\n      this.tenant.image = res.data;\n    },\n    beforeAvatarUpload(file) {\n      const isJPG = file.type === \"image/jpeg\";\n      const isLt2M = file.size / 1024 / 1024 < 2;\n      if (!isJPG) {\n        this.$message.error(\"上传头像图片只能是 JPG 格式!\");\n      }\n      if (!isLt2M) {\n        this.$message.error(\"上传头像图片大小不能超过 2MB!\");\n      }\n      return isJPG && isLt2M;\n    },\n  },\n\n\n  watch: {\n    entrydate(val) {\n      if (val && val.length >= 2) {\n        this.beginTime = val[0];\n        this.endTime = val[1];\n      } else {\n        this.beginTime = \"\";\n        this.endTime = \"\";\n      }\n    },\n  },\n};\n</script>\n<style>\n.avatar-uploader .el-upload {\n  border: 1px dashed #d9d9d9;\n  border-radius: 6px;\n  cursor: pointer;\n  position: relative;\n  overflow: hidden;\n}\n.avatar-uploader .el-upload:hover {\n  border-color: #409eff;\n}\n.avatar-uploader-icon {\n  font-size: 28px;\n  color: #8c939d;\n  width: 100px;\n  height: 100px;\n  line-height: 100px;\n  text-align: center;\n}\n.avatar {\n  width: 100px;\n  height: 100px;\n  display: block;\n}\n</style>\n\n"]}]}